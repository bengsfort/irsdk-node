{
  "name": "irsdk-node",
  "version": "4.0.3",
  "author": {
    "name": "Matt Bengston",
    "email": "bengsfort@gmail.com",
    "url": "http://bengsfort.dev/"
  },
  "bugs": {
    "email": "bengsfort@gmail.com",
    "url": "https://github.com/bengsfort/irsdk-node/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "package.json",
    "./tsconfig.json",
    "./src",
    "./dist",
    "./README.md"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
    "build": "tsc",
    "watch": "tsc --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint ./src --color",
    "run-debug": "node ./dist/__tests__/sandbox.js",
    "dump-data": "node ./dist/__tests__/dump-data.js"
  },
  "packageManager": "pnpm@10.17.1",
  "optionalDependencies": {
    "@irsdk-node/native": "workspace:^"
  },
  "dependencies": {
    "@irsdk-node/types": "workspace:^",
    "js-yaml": "^4.1.0",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.18.126",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-jest": "^27.4.4",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.4",
    "node-notifier": "^10.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.2"
  }
}
