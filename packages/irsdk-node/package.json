{
  "name": "irsdk-node",
  "version": "4.0.4",
  "author": "Matt Bengston <bengsfort@gmail.com> (https://bengsfort.dev/)",
  "bugs": {
    "email": "bengsfort@gmail.com",
    "url": "https://github.com/bengsfort/irsdk-node/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "package.json",
    "./dist",
    "./README.md",
    "./CHANGELOG.md"
  ],
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
    "build": "pnpm run \"/^build:.*/\"",
    "build:ts": "node ./esbuild.js",
    "build:types": "tsc --emitDeclarationOnly",
    "watch": "pnpm run \"/^watch:.*/\"",
    "watch:ts": "node ./esbuild.js --watch",
    "watch:types": "tsc --watch --emitDeclarationOnly",
    "lint": "eslint ./src --color",
    "run-debug": "node ./dist/__tests__/sandbox.js",
    "dump-data": "node ./dist/__tests__/dump-data.js"
  },
  "packageManager": "pnpm@10.17.1",
  "optionalDependencies": {
    "@irsdk-node/native": "workspace:^"
  },
  "dependencies": {
    "@irsdk-node/types": "workspace:^",
    "js-yaml": "^4.1.0",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.18.126",
    "esbuild": "^0.25.11",
    "node-notifier": "^10.0.0",
    "typescript": "^5.9.3"
  }
}