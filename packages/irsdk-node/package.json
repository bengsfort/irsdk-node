{
  "name": "irsdk-node",
  "version": "4.0.4",
  "author": {
    "name": "Matt Bengston",
    "email": "bengsfort@gmail.com",
    "url": "http://bengsfort.dev/"
  },
  "bugs": {
    "email": "bengsfort@gmail.com",
    "url": "https://github.com/bengsfort/irsdk-node/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "package.json",
    "./tsconfig.json",
    "./src",
    "./dist",
    "./README.md"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
    "build": "tsc",
    "watch": "tsc --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint ./src --color",
    "run-debug": "node ./dist/__tests__/sandbox.js",
    "dump-data": "node ./dist/__tests__/dump-data.js"
  },
  "packageManager": "pnpm@10.17.1",
  "optionalDependencies": {
    "@irsdk-node/native": "workspace:^"
  },
  "dependencies": {
    "@irsdk-node/types": "workspace:^",
    "js-yaml": "^4.1.0",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.18.126",
    "node-notifier": "^10.0.0",
    "typescript": "^5.9.3"
  }
}