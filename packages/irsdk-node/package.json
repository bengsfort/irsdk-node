{
  "name": "irsdk-node",
  "version": "3.0.0",
  "author": {
    "name": "Matt Bengston",
    "email": "bengsfort@gmail.com"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "package.json",
    "./tsconfig.json",
    "./src",
    "./dist",
    "./README.md"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint ./src --ext .js,.ts",
    "prepack": "yarn build",
    "run-debug": "node ./dist/__tests__/sandbox.js",
    "dump-data": "node ./dist/__tests__/dump-data.js"
  },
  "packageManager": "yarn@3.1.0",
  "preferUnplugged": false,
  "optionalDependencies": {
    "@irsdk-node/native": "workspace:*"
  },
  "dependencies": {
    "@irsdk-node/types": "workspace:^",
    "js-yaml": "^4.1.0",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.11.12",
    "babel-jest": "^27.4.4",
    "jest": "^27.4.4",
    "node-notifier": "^10.0.0",
    "typescript": "^4.5.2"
  }
}
