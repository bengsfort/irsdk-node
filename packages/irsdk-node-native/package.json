{
  "name": "@irsdk-node/native",
  "version": "4.0.2",
  "type": "module",
  "author": {
    "name": "Matt Bengston",
    "email": "bengsfort@gmail.com",
    "url": "http://bengsfort.dev/"
  },
  "bugs": {
    "email": "bengsfort@gmail.com",
    "url": "https://github.com/bengsfort/irsdk-node/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "package.json",
    "./build",
    "./dist",
    "./lib",
    "./src",
    "./scripts",
    "./binding.gyp",
    "./README.md"
  ],
  "main": "./dist/index.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "clean:ts": "rm -rf dist *.tsbuildinfo",
    "clean:cpp": "node-gyp clean",
    "clean": "pnpm run \"/^clean:.*/\"",
    "configure": "pnpm run clean:cpp && node-gyp configure",
    "build": "pnpm run configure && pnpm run \"/^build:(ts|cpp)$/\"",
    "build:ts": "tsc --build --force",
    "build:cpp": "pnpm run \"/^build:cpp\\-.*/\"",
    "build:cpp-release": "node-gyp build",
    "build:cpp-debug": "node-gyp build --debug",
    "watch": "tsc --watch",
    "lint": "eslint",
    "check-types": "tsc --noEmit",
    "generate-types": "node ./scripts/generate-var-types.js"
  },
  "prettier": "@bengsfort/eslint-config-flat/prettier.config.js",
  "packageManager": "pnpm@10.17.1",
  "os": [
    "win32"
  ],
  "dependencies": {
    "@irsdk-node/types": "workspace:^",
    "bindings": "^1.5.0",
    "node-addon-api": "^8.2.1",
    "node-gyp": "^10.2.0"
  },
  "devDependencies": {
    "@bengsfort/eslint-config-flat": "^0.2.5",
    "eslint": "^9.36.0",
    "node-notifier": "^10.0.0",
    "typescript": "^5.9.2"
  }
}
