{
  "name": "@irsdk-node/native",
  "version": "4.1.1",
  "author": {
    "name": "Matt Bengston",
    "email": "bengsfort@gmail.com",
    "url": "http://bengsfort.dev/"
  },
  "bugs": {
    "email": "bengsfort@gmail.com",
    "url": "https://github.com/bengsfort/irsdk-node/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "package.json",
    "./dist",
    "./lib",
    "./index.js",
    "./index.d.ts",
    "./prebuilds",
    "./scripts",
    "./binding.gyp",
    "./README.md"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    }
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "install": "node-gyp-build",
    "clean:ts": "rm -rf dist *.tsbuildinfo",
    "clean:cpp": "rm -rf ./prebuilds ./build",
    "clean": "pnpm run \"/^clean:.*/\"",
    "build": "pnpm run clean && pnpm run \"/^build:(ts|cpp)$/\"",
    "build:ts": "tsc --build --force",
    "build:cpp": "prebuildify --napi --electron-compat",
    "watch": "tsc --watch",
    "lint": "eslint ./src/**/*.ts --color",
    "check-types": "tsc --noEmit",
    "generate-types": "node ./scripts/generate-var-types.js"
  },
  "packageManager": "pnpm@10.17.1",
  "dependencies": {
    "@irsdk-node/types": "workspace:^",
    "bindings": "^1.5.0",
    "js-yaml": "^4.1.0",
    "node-addon-api": "^8.2.1",
    "node-gyp": "^10.2.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "node-notifier": "^10.0.0",
    "prebuildify": "^6.0.1",
    "typescript": "^5.9.3"
  }
}